<Shell
    x:Class="FinalProjectMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinalProjectMaui"
    Shell.FlyoutBehavior="Disabled"
    Title="FinalProjectMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Create Event"
        ContentTemplate="{DataTemplate local:EventCreationPage}"
        Route="EventCreationPage" />

</Shell>

